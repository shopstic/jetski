{
  "name": "@wok/jetski",
  "version": "1.9.0",
  "exports": {
    "./types": "./src/types.ts"
  },
  "publish": {
    "include": [
      "./src",
      "./deno.json",
      "./README.md",
      "./LICENSE"
    ]
  },
  "tasks": {
    "check_all": "deno check **/*.ts"
  },
  "fmt": {
    "include": [
      "build.ts",
      "src/",
      "examples/"
    ],
    "exclude": [
      "**/.secrets/*"
    ],
    "lineWidth": 120
  },
  "lint": {
    "include": [
      "src/",
      "examples/"
    ],
    "exclude": [
      "**/.secrets/*"
    ],
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  }
}
