{
  "name": "@wok/jetski",
  "version": "2.0.2",
  "exports": {
    "./types": "./src/types.ts",
    ".": "./src/app.ts"
  },
  "publish": {
    "include": [
      "./src",
      "./deno.json",
      "./README.md",
      "./LICENSE"
    ]
  },
  "fmt": {
    "include": [
      "build.ts",
      "src/",
      "examples/"
    ],
    "exclude": [
      "**/.secrets/*"
    ],
    "lineWidth": 120
  },
  "lint": {
    "include": [
      "src/",
      "examples/"
    ],
    "exclude": [
      "**/.secrets/*"
    ],
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/async": "jsr:@std/async@^1.0.10",
    "@std/fmt": "jsr:@std/fmt@^1.0.5",
    "@std/fs": "jsr:@std/fs@^1.0.11",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/yaml": "jsr:@std/yaml@^1.0.5",
    "@wok/schema": "jsr:@wok/schema@^2.2.0",
    "@wok/utils": "jsr:@wok/utils@^4.2.9"
  }
}
