{
  "name": "@wok/jetski",
  "version": "1.9.2",
  "exports": {
    "./types": "./src/types.ts"
  },
  "publish": {
    "include": [
      "./src",
      "./deno.json",
      "./README.md",
      "./LICENSE"
    ]
  },
  "tasks": {
    "check_all": "deno check **/*.ts"
  },
  "fmt": {
    "include": [
      "build.ts",
      "src/",
      "examples/"
    ],
    "exclude": [
      "**/.secrets/*"
    ],
    "lineWidth": 120
  },
  "lint": {
    "include": [
      "src/",
      "examples/"
    ],
    "exclude": [
      "**/.secrets/*"
    ],
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/async": "jsr:@std/async@^1.0.0",
    "@std/fmt": "jsr:@std/fmt@^0.225.6",
    "@std/fs": "jsr:@std/fs@^0.229.3",
    "@std/path": "jsr:@std/path@^1.0.1",
    "@std/yaml": "jsr:@std/yaml@^0.224.3",
    "@wok/utils": "jsr:@wok/utils@^1.3.5"
  }
}
